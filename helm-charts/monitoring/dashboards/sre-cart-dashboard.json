{
  "id": null,
  "title": "SRE - Cart Service Dashboard",
  "tags": [
    "sre",
    "cart",
    "ecommerce",
    "conversion"
  ],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Cart Service Availability",
      "type": "stat",
      "targets": [
        {
          "expr": "avg_over_time(up{job=\"cart\"}[5m]) * 100",
          "legendFormat": "Availability %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              {
                "color": "red",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 99
              },
              {
                "color": "green",
                "value": 99.9
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Cart Operation Response Time (95th percentile)",
      "type": "stat",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(cart_operation_duration_seconds_bucket[5m]))",
          "legendFormat": "95th Percentile (s)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 1
              },
              {
                "color": "red",
                "value": 2
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 6,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Cart Error Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(cart_operations_total{status=\"error\"}[5m]) / rate(cart_operations_total[5m]) * 100",
          "legendFormat": "Error Rate %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": 0
              },
              {
                "color": "yellow",
                "value": 5
              },
              {
                "color": "red",
                "value": 10
              }
            ]
          }
        }
      },
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 4,
      "title": "Active Carts",
      "type": "stat",
      "targets": [
        {
          "expr": "cart_active_carts",
          "legendFormat": "Active Carts"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 18,
        "y": 0
      }
    },
    {
      "id": 5,
      "title": "Cart Operations Rate",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(cart_operations_total[5m])",
          "legendFormat": "{{operation}} ({{status}})"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 6,
      "title": "Cart Errors by Type",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(cart_errors_total[5m])",
          "legendFormat": "{{operation}}: {{error_type}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "id": 7,
      "title": "Items per Cart Distribution",
      "type": "heatmap",
      "targets": [
        {
          "expr": "rate(cart_items_per_cart_bucket[5m])",
          "legendFormat": "{{le}} items"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 8,
      "title": "Cart Value Distribution",
      "type": "heatmap",
      "targets": [
        {
          "expr": "rate(cart_value_total_bucket[5m])",
          "legendFormat": "${{le}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      }
    },
    {
      "id": 9,
      "title": "Cart Abandonment Rate",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(cart_abandonment_total[5m])",
          "legendFormat": "{{reason}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      }
    },
    {
      "id": 10,
      "title": "Redis Connection Status",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(cart_redis_connections_total[5m])",
          "legendFormat": "{{status}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "30s"
}
